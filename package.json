  "description": "This is a simple search field component for react",
  "main": "dist/SearchField.js",
  "scripts": {
    "build:component": "babel src/components --out-dir dist",
    "build:docs": "webpack",
    "build": "npm run build:component && npm run build:docs",
    "dev": "npm run build:docs && webpack-dev-server"
